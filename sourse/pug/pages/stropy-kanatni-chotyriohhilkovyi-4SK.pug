block variables
	- let  mainPage= 'main-page' 
	- let  bg= '04.png'

block title
	- let  title= 'catalog'
	title= title
block pagecontent
	+header()
		+top-nav()
	main
		+sProdCard({
			breadcrumbs: [
				{href: './', txt: 'Головна',},
				{href: './catalog.html', txt: 'Каталог',},
				{
					href: './catalog.html?category=stropy-kanatni',
					txt: 'Стропи канатні',
				},
				{
					href: '',
					txt: 'Строп канатний 4СК',
				},
			],
			title: 'Строп канатний 4СК',
			imgs: [
				'stropy-kanatni-3.jpg',
				'stropy-kanatni-hook.jpg',
			],
			chars: [
				{
					key: 'Строп канатний чотирьохгілковий',
					val: '',
				},
				{
					key: 'Коефіцієнт запасу міцності',
					val: '6:1;',
				},
				{
					key: 'Матеріал',
					val: 'світлі канати без покриття ГОСТ 7668-80, 2688-80, ланки G80;',
				},
				{
					key: 'Температурний діапазон',
					val: 'від -40 до +100 °С;',
				},
				{
					key: 'Стандарт',
					val: 'ДСТУ EN 13414-1:2018, ДСТУ Б В.2.8-10-98;',
				},
				{
					key: 'Гарантія',
					val: '6 місяців.',
				},
			],
			pills: ['1,6', '2', '2,5', '4', '5', '6,3', '8', '10', '12,5', '16', '20', '25'],
			tableTxt: 'ТЕХНІЧНІ ХАРАКТЕРИСТИКИ КАНАТНИХ СТРОП (ПАВУКІВ) 4СК',
			tableImg: 'stropy-kanatni-3-table.png',
		})
			//- content here
			p Строп 4СК виготовляється шляхом обтискування каната алюмінієвою втулкою і може використовуватися з комплектуючими відповідно до стандартів. Цей тип стропа використовується, коли на вантажі є спеціальні місця для закріплення гаків. Вартість стропа 4СК залежить від вантажопідйомності та довжини виробу, але він є дешевшим, ніж його ланцюговий аналог.
			h6 Сфери застосування:
			p Використання канатних стропів 4СК можливе в різних виробничих умовах, включаючи будівництво, навантажувально-розвантажувальні роботи та такелажні роботи в морських і річкових портах.
		+sOther()
			//-other here
			+sOtherItem({
				picName: 'stropy-kanatni-1',
				title: 'Одногілковий 1СК',
				pills: ['0,63 т', '0,8 т', '1 т', '1,6 т', '2 т', '2,5 т', '3,2 т', '4 т', '5 т', '6,3 т', '8 т', '10 т', '12,5 т'],
				link: 'stropy-kanatni-odnohilkovyi-1SK.html',
			})
			+sOtherItem({
				picName: 'stropy-kanatni-2',
				title: 'Двогілковий 2СК',
				pills: ['0,8 т', '1 т', '1,6 т', '2 т', '2,5 т', '3,2 т', '4 т', '5 т', '6,3 т', '8 т', '10 т', '12,5 т', '16 т'],
				link: 'stropy-kanatni-dvohilkovyi-2SK.html',
			})
			+sOtherItem({
				picName: 'stropy-kanatni-4',
				title: 'Двопетльовий СКП (заплітання)',
				pills: ['0,63 т', '0,8 т', '1,25 т', '1,6 т', '2 т', '2,5 т', '2,8 т', '3,2 т', '4 т', '5 т', '6,3 т', '8 т', '10 т', '12,5 т', '16 т', '20 т'],
				link: 'stropy-kanatni-Dvopetliovyj-SKP-(zaplitannya).html',
			})
			+sOtherItem({
				picName: 'stropy-kanatni-5',
				title: 'Двопетльовий СКП (втулка)',
				pills: ['0,63 т', '0,8 т', '1,25 т', '1,6 т', '2 т', '2,5 т', '2,8 т', '3,2 т', '4 т', '5 т', '6,3 т', '8 т', '10 т', '12,5 т', '16 т', '20 т'],
				link: 'stropy-kanatni-Dvopetliovyj-SKP-(vtulka).html',
			})
	+footer()